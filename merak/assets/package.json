{
  "name": "merak",
  "version": "1.0.0",
  "description": "merak theme",
  "main": "index.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clean": "rm -rf build/*",
    "svg": "svgr -d src/js/icons src/icons",
    "lint": "eslint src/js/*",
    "lint:fix": "eslint --fix src/js/*",
    "lint-css": "stylelint 'src/sass/**/*.scss'",
    "lint-css:fix": "npm run lint-css -- --fix",
    "precommit": "npm run lint-css:fix && npm run lint:fix && npm run prod"
  },
  "author": "Oleh",
  "license": "ISC",
  "dependencies": {
    "copy-webpack-plugin": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@wordpress/base-styles": "^3.5.2",
    "@wordpress/block-editor": "^6.1.3",
    "@wordpress/blocks": "^9.1.3",
    "@wordpress/components": "^14.1.2",
    "@wordpress/compose": "^4.1.1",
    "@wordpress/data": "^5.1.1",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.1.4",
    "@wordpress/element": "^3.1.1",
    "@wordpress/eslint-plugin": "^9.0.6",
    "@wordpress/hooks": "^3.1.1",
    "@wordpress/i18n": "^4.1.1",
    "@wordpress/icons": "^4.0.1",
    "@wordpress/server-side-render": "^2.1.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.5",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "sass-loader": "^12.0.0",
    "sass-mq": "^5.0.1",
    "style-loader": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  }
}
